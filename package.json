{
  "name": "labelizing",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "author": "Maxime Peeters",
  "skpm": {
    "name": "labelizING",
    "manifest": "src/manifest.json",
    "main": "labelizing.sketchplugin",
